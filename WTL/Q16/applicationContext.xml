<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-beans.xsd">
		
	<bean id="emp" class="com.Q16.Employee" scope="prototype">
		<property name="eId" value="101"></property>
		<property name="eName" value="ABC"></property>
		<property name="eSalary" value="1000000"></property>
		<property name="eDept" value="SALES"></property>
	</bean>
</beans>
